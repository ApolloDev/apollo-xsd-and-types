<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>edu.pitt.apollo</groupId>
  <artifactId>xsd-types</artifactId>
  <version>3.1.0</version>
  <name>Apollo XSD types</name>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
      <comments>A business-friendly OSS license</comments>
    </license>
  </licenses>
  <prerequisites>
    <maven>3.0</maven>
  </prerequisites>
  <scm>
    <developerConnection>scm:git:https://github.com/ApolloDev/apollo-xsd-and-types</developerConnection>
    <tag>3.1.0</tag>
  </scm>
  <distributionManagement>
    <repository>
      <id>nexus</id>
      <name>RODS Nexus</name>
      <url>https://internal.rods.pitt.edu/nexus/content/repositories/apollo-releases/</url>
    </repository>
    <snapshotRepository>
      <id>nexus</id>
      <name>RODS Nexus</name>
      <url>https://internal.rods.pitt.edu/nexus/content/repositories/apollo-snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <cxf.version>2.7.3</cxf.version>
  </properties>
  <dependencies>
    <dependency>
      <groupId>asm</groupId>
      <artifactId>asm</artifactId>
      <version>3.3.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.google.code.javaparser</groupId>
      <artifactId>javaparser</artifactId>
      <version>1.0.11</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.sun.xml.bind</groupId>
      <artifactId>jaxb-impl</artifactId>
      <version>2.2.6</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>commons-beanutils</groupId>
      <artifactId>commons-beanutils</artifactId>
