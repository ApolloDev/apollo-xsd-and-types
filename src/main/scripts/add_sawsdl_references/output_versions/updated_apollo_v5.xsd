        </complexContent>
    </complexType>
    <complexType name="Ecosystem" sawsdl:modelReference="http://purl.obolibrary.org/obo/APOLLO_SV_00000097">
        <annotation>
            <documentation>
                The sum of all biotic and abiotic entities in a geographical region.
            </documentation>
        </annotation>
    </complexType>

